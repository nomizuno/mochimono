<%= render 'partial/navbar' %>

 <div class= "container">
    <div class = "row">
      	<% @items.each do |item| %>
          <div class="col-xs-12 col-sm-6 col-md-4">
            <div class = "item-index">
              <%= image_tag item.image, class: "border rounded w-25" %>
              <h2>@<%= item.name %></h2>
            </div>
          </div>

        <% end %>
     </div>
  </div>
